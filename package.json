{
  "name": "no-polling-example",
  "version": "0.1.0",
  "description": "experiment with clients that don't poll and are resilient to poor network conditions",
  "main": "index.js",
  "scripts": {
    "browserify": "browserify -d -t rollupify -t babelify -e www/index.js | exorcist dist/index.js.map > dist/index.js",
    "build:dist": "npm run build:html && npm run browserify",
    "build:html": "cp -v www/index.html dist/",
    "eslint": "eslint --fix .",
    "server": "node ./server/index.js",
    "start": "npm run build:dist && npm run server",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blinkmobile/no-polling-example.git"
  },
  "keywords": [],
  "author": "",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/blinkmobile/no-polling-example/issues"
  },
  "homepage": "https://github.com/blinkmobile/no-polling-example#readme",
  "engines": {
    "node": ">=5",
    "npm": ">=3"
  },
  "devDependencies": {
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "eslint": "^2.7.0",
    "eslint-config-semistandard": "^6.0.1",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-standard": "^1.3.2",
    "exorcist": "^0.4.0",
    "rollupify": "^0.2.0"
  },
  "dependencies": {
    "busmq": "1.4.0",
    "hapi": "13.3.0",
    "inert": "3.2.0"
  }
}
